{
  "name": "wechat-auth-proxy",
  "version": "0.0.5",
  "description": "wechat oauth proxy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\.(test|spec)\\.(tsx?)$",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenwei1202/wechat-auth-proxy.git"
  },
  "author": "vincent wen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wenwei1202/wechat-auth-proxy/issues"
  },
  "homepage": "https://github.com/wenwei1202/wechat-auth-proxy#readme",
  "dependencies": {
    "co-wechat-oauth": "^2.0.1",
    "immutable": "^4.0.0-rc.9",
    "koa-router": "^7.3.0",
    "lodash": "^4.17.4",
    "superstruct": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/koa": "^2.0.39",
    "@types/koa-router": "^7.0.27",
    "@types/koa-session": "^3.0.5",
    "@types/lodash": "^4.14.116",
    "@types/node": "^8.0.34",
    "@types/superstruct": "^0.5.0",
    "jest": "^22.0.4",
    "koa": "^2.3.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.1.0",
    "typescript": "2.9"
  }
}
